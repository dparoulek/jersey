<assembly>
    <formats>
        <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
            <directory>target/pom-file</directory>
            <outputDirectory></outputDirectory>
        </fileSet>
        <fileSet>
            <directory>../..</directory>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory></outputDirectory>
            <excludes>
                <exclude>**/target/**</exclude>
            </excludes>
            <includes>
                <include>jersey-core/**</include>
                <include>jersey-server/**</include>
                <include>jersey-client/**</include>
                <include>jersey-atom/**</include>
                <include>jersey-json/**</include>
                <include>jersey-fastinfoset/**</include>
            </includes>
        </fileSet>
        <fileSet>
          <directory>../../contribs</directory>
          <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory></outputDirectory>
            <excludes>
                <exclude>**/target/**</exclude>
            </excludes>
            <includes>
                <include>jersey-multipart/**</include>
            </includes>
        </fileSet>
    </fileSets>
    <files>
        <file>
            <source>../../license.html</source>
            <outputDirectory></outputDirectory>
        </file>
    </files>
</assembly>
